Algoritmo "ExerccioParte1-Consumo"
Var

   distancia, combustivel, consumoMedio : real

Inicio

      escreval("Distancia percorrida:")
      leia(distancia)
      escreval("Combustível gasto:")
      leia(combustivel)
      
      escreval
      
      consumoMedio <- distancia / combustivel
      
      
      escreval("Consumo médio:", consumoMedio:2:3)

Fimalgoritmo